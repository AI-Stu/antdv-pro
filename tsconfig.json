{
  "extends": "@cosing/tsconfig/web.json",
  "compilerOptions": {
    "target": "ES2020",

    // paths
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "~#/*": ["src/enums/*"]
    },
    /* Bundler mode */
    "allowImportingTsExtensions": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "types/**/*.d.ts"]
  // "references": [{ "path": "./tsconfig.node.json" }]
}
