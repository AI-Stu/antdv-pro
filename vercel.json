{
  "rewrites": [
    {
      "source": "/api",
      "destination": "https://api.cosing-plus.com"
    },
    {
      "source": "/api/",
      "destination": "https://api.cosing-plus.com/"
    },
    {
      "source": "/api/:match*",
      "destination": "https://api.cosing-plus.com/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
